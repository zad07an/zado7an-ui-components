{
  "name": "zado7an-ui-components",
  "private": false,
  "version": "0.0.3",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
